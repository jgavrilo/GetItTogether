{
  "manifest_version": 3,
  "name": "My Chrome Tool Set",
  "version": "1.2.1",
  "description": "A tool set for chrome",
  "permissions": [
    "identity"
  ],
  "action": {
    "default_popup": "src/html/popup.html",
    "default_icon": {
      "16": "assets/images/tools-icon16.png",
      "48": "assets/images/tools-icon48.png",
      "128": "assets/images/tools-icon128.png"
    }
  },
  "icons": {
    "16": "assets/images/tools-icon16.png",
    "48": "assets/images/tools-icon48.png",
    "128": "assets/images/tools-icon128.png"
  }
}
