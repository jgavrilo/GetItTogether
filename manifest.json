{
  "manifest_version": 3,
  "name": "My Chrome Tool Set",
  "version": "1.0",
  "description": "A tool set for chrome",
  "permissions": ["alarms", "activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
 }
 